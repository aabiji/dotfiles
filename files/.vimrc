set clipboard+=unnamedplus
set mouse=a 
set termguicolors
set number
nnoremap yy "+yy
vnoremap y "+y
nnoremap p "+p
nnoremap P "+P
nnoremap <C-h> <C-w>h
nnoremap <C-j> <C-w>j
nnoremap <C-k> <C-w>k
nnoremap <C-l> <C-w>l
nnoremap <C-m> :vsplit<CR>
nnoremap <C-n> :split<CR>
nnoremap 0 _
nnoremap _ 0
set noerrorbells
set novisualbell
set t_vb=
